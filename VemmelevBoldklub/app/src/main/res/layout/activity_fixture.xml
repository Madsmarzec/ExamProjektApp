<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.madsm.vemmelevboldklub.FixtureActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_blue_bright"
        android:text="FixutreActivity"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="274dp" />

    <Button
        android:id="@+id/btnGetFix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="getJSON"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView3"
        android:text="Hent Kampe" />

    <Button
        android:id="@+id/btnParseFix"
        android:layout_width="wrap_content"
        android:onClick="parseJSON"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btnGetFix"
        android:layout_alignBottom="@+id/btnGetFix"
        android:layout_toEndOf="@+id/btnGetFix"
        android:layout_toRightOf="@+id/btnGetFix"
        android:text="Parse Kampe" />

    <TextView
        android:id="@+id/txtFixView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/btnGetFix"
        android:text="JSON Appears" />
</RelativeLayout>
