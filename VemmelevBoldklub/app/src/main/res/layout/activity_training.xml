<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.madsm.vemmelevboldklub.TrainingActivity">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_purple"
        android:text="TrainingSession"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="129dp" />

    <Button
        android:id="@+id/btnGetJSON"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="getJSON"
        android:text="Get JSON"
        android:layout_below="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/txtJSON"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JSON appears here"
        android:layout_below="@+id/btnGetJSON"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btnParseJSON"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="parseJSON"
        android:text="Parse JSON"
        android:layout_below="@+id/textView6"
        android:layout_toRightOf="@+id/textView6"
        android:layout_toEndOf="@+id/textView6" />
</RelativeLayout>
